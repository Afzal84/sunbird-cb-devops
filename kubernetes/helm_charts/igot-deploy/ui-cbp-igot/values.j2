envjs: |-
 (function (window) {
    window["env"] = window["env"] || {};
    // Environment variables
    window["env"]["debug"] = '';
    window["env"]["sitePath"] = '{{ cbp_domain_name }}';
    window["env"]["organisation"] = '{{ igot_organisation }}';
    window["env"]["framework"] = '{{ igot_framework }}'
    window["env"]["karmYogi"] = '{{ igot_portal_name }}/'	
    window["env"]["channelId"] = '{{ igot_channelid }}'
    window["env"]["azureHost"] = '{{ igot_azurehost }}'
    window["env"]["azureBucket"] = '{{ igot_azurebucket }}'
    window["env"]["azureOldHost"] = '{{ igot_azureoldhost }}'
    window["env"]["azureOldBuket"] = '{{ igot_azureoldbuket }}'
    window["env"]["portalRoles"] = '{{ igot_cbprules }}';
    window["env"]["scromContentEndpoint"] = 'assets/public/content/html/'
    window["env"]["cbpPortal"] = 'https://{{ cbp_domain_name }}/'

 })(this);
